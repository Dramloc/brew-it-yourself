{
  "private": true,
  "name": "brew-it-yourself",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --template src/template.html",
    "serve": "npm run build && preact serve",
    "dev": "preact watch --template src/template.html",
    "lint": "eslint src/**/*.{jsx,js}",
    "test": "jest",
    "csscomb": "csscomb src"
  },
  "devDependencies": {
    "babel-plugin-async-to-promises": "^1.0.5",
    "csscomb": "^4.2.0",
    "eslint": "^4.19.1",
    "eslint-config-synacor": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "if-env": "^1.0.4",
    "jest": "^23.3.0",
    "node-sass": "^4.9.2",
    "preact-cli": "^2.2.1",
    "preact-render-spy": "^1.3.0",
    "sass-loader": "^7.0.3"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "normalizr": "^3.2.4",
    "preact": "^8.2.9",
    "preact-compat": "^3.18.0",
    "preact-helmet": "^4.0.0-alpha-3",
    "preact-redux": "^2.0.3",
    "preact-router": "^2.6.1",
    "preact-router-redux": "^4.1.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1"
  }
}
